printUntil(30)

func printUntil(%n, %curr = 1) {
    if curr == n {
        return
    }

    print(fibonacci(curr))
    printUntil(n, curr + 1)
}

func fibonacci(%n) {
    if n == 0 {
        return 0
    } else if n ==1 {
        return 1
    } else {
        return fibonacci(n-1) + fibonacci(n-2)
    }
}